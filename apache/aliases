alias load-apache="sudo launchctl load /System/Library/LaunchDaemons/org.apache.httpd.plist"
alias unload-apache="sudo launchctl unload /System/Library/LaunchDaemons/org.apache.httpd.plist"
alias reload-apache="unload-apache && load-apache"

alias tal="tail -f /private/var/log/apache2/access_log"
alias tel="tail -f /private/var/log/apache2/error_log"

